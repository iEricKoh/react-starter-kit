{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-1",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-async-to-generator"
    ]
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "NODE_ENV=production babel-node server.js",
    "dev": "nodemon --exec babel-node --presets=es2015 -- ./server.js",
    "build": "npm run clean && webpack --progress --env.production --optimize-minimize"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "browser-sync": "^2.17.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.24.1",
    "koa-webpack": "^0.1.9",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.1",
    "postcss-scss": "^0.4.0",
    "postcss-smart-import": "^0.5.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.10"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "sequelize": "^3.25.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-combine-routers": "^1.0.0",
    "koa-logger": "^1.3.0",
    "koa-router": "^7.0.1",
    "koa-send": "^3.2.0",
    "koa-static": "^2.0.0",
    "koa-static-cache": "^3.1.7",
    "pg": "^6.1.0",
    "axios": "^0.15.2"
  }
}
